
{
  "manifest_version": 3,
  "name": "Hello Extensions",
  "description": "COEN 296A A3",
  "version": "2.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "helloworld.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"],
      "css": ["timerStyles.css"]
    }
  ],
  "background": {
    "service_worker": "background-1.js"
  },
  "permissions": ["storage", "tabs", "activeTab", "scripting", "webNavigation", "storage",
  "webRequest"]
}
